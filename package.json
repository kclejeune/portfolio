{
  "name": "kclejeune.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kclejeune/portfolio.git"
  },
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "start": "node build",
    "deploy": "git subtree push --prefix build origin gh-pages",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "upgrade": "npm-check-updates -u && npm install"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.10",
    "@sveltejs/kit": "^1.0.0-next.267",
    "@tailwindcss/typography": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.17",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "npm-check-updates": "^12.3.0",
    "postcss": "^8.4.6",
    "postcss-load-config": "^3.1.3",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.10.3",
    "svelte-scrollto-element": "^0.7.0",
    "svelte-seo": "^1.4.0",
    "tailwindcss": "^3.0.22",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "type": "module"
}
