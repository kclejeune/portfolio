{
  "name": "kclejeune.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kclejeune/portfolio.git"
  },
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "start": "node build",
    "deploy": "git subtree push --prefix build origin gh-pages",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "upgrade": "npm-check-updates -u && npm install"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "npm-check-updates": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-load-config": "^3.1.4",
    "prettier": "~2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "svelte-scrollto-element": "^0.7.0",
    "svelte-seo": "^1.4.0",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "type": "module"
}
