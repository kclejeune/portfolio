{
  "name": "kclejeune.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/kclejeune/portfolio.git"
  },
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "svelte-kit build",
    "start": "node build",
    "deploy": "git subtree push --prefix build origin gh-pages",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.19",
    "@sveltejs/kit": "^1.0.0-next.169",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.6",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^2.2.15",
    "tslib": "^2.0.0",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "svelte-scrollto": "^0.2.0"
  }
}
